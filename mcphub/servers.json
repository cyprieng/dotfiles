{
  "mcpServers": {
    "context7": {
      "args": ["-y", "@upstash/context7-mcp@latest"],
      "autoApprove": ["resolve-library-id", "get-library-docs"],
      "command": "npx",
      "disabled": false
    },
    "excel": {
      "args": ["excel-mcp-server", "stdio"],
      "command": "uvx"
    },
    "git": {
      "args": ["mcp-server-git"],
      "command": "uvx",
      "disabled": false
    },
    "github": {
      "url": "https://api.githubcopilot.com/mcp/",
      "headers": {
        "Authorization": "Bearer ${cmd: echo $GITHUB_PERSONAL_ACCESS_TOKEN}"
      }
    },
    "markitdown": {
      "command": "markitdown-mcp",
      "disabled": false
    },
    "sequential-thinking": {
      "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"],
      "command": "npx"
    },
    "time": {
      "args": ["mcp-server-time"],
      "command": "uvx"
    }
  },
  "nativeMCPServers": []
}
